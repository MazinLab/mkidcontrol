[Unit]
Description=MKIDControl Quench Agent
After=currentduino.service
Wants=currentduino.service

[Install]
WantedBy=mkidcontrol.service
WantedBy=sim960.service

[Service]
Type=simple
ExecStart=/home/mazinlab/anaconda3/envs/control/bin/quenchAgent.py
WorkingDirectory=/home/mazinlab/mkidcontrol
User=mazinlab
RestartSec=100ms
Restart=always